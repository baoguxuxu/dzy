<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.meiguo.goods.dao.CategoryDao">

	<select id="get" resultType="com.meiguo.goods.domain.CategoryDO">
		select `id`,`name`,jiangguo_flag,`create_time`,`create_by`,`update_time`,`update_by`,`remarks` from product_category where id = #{value}
	</select>
</mapper>